extends layout

block content
  h1= title
  - const image_names = ['tolly.jpg', 'bolly.jpg', 'kolly.jpg', 'entertain.jpg', 'movie.jpg']
  - for (let i = 0; i < 3; i++)
    - const randomIndex = Math.floor(Math.random() * 5)
    img(src=`/images/${image_names[randomIndex]}`, alt=image_names[randomIndex], height="300", width="300")